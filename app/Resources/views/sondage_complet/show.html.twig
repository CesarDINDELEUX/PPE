{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sondage_complet</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sondage_Complet.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if sondage_Complet.date %}{{ sondage_Complet.date|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Istermine</th>
                <td>{% if sondage_Complet.isTermine %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('sondage_complet_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('sondage_complet_edit', { 'id': sondage_Complet.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
