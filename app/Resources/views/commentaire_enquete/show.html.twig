{% extends 'base.html.twig' %}

{% block body %}
    <h1>Commentaire_enquete</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commentaire_Enquete.id }}</td>
            </tr>
            <tr>
                <th>Commentaire</th>
                <td>{{ commentaire_Enquete.commentaire }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if commentaire_Enquete.date %}{{ commentaire_Enquete.date|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('commentaire_enquete_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('commentaire_enquete_edit', { 'id': commentaire_Enquete.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
